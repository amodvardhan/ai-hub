{
  "name": "enterprise-react-app",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:azuread": "vite --mode azuread",
    "build": "tsc && vite build",
    "build:azuread": "tsc && vite build --mode production.azuread",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "lint:check": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "preview:azuread": "vite preview --mode production.azuread",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rm -rf dist node_modules/.vite",
    "check": "npm run type-check && npm run lint:check && npm run format:check"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.25.1",
    "@azure/msal-react": "^3.0.20",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.3",
    "@mui/material": "^6.1.3",
    "@tanstack/react-query": "^5.59.8",
    "@tanstack/react-query-devtools": "^5.59.8",
    "axios": "^1.7.7",
    "formik": "^2.4.6",
    "i18next": "^23.15.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "notistack": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-i18next": "^15.0.2",
    "react-router-dom": "^6.27.0",
    "yup": "^1.7.1",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^9.12.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-unused-imports": "^4.3.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.8"
  }
}