# Azure AD Environment Variables for Development with Mock Azure AD
# ============================================
# DEVELOPMENT - AZURE AD AUTHENTICATION WITH MOCK AZURE AD
# ============================================
# Uses mock Azure AD for authentication - no real Azure AD backend needed
# Perfect for frontend development with Azure AD flows
VITE_AUTH_PROVIDER=azure-ad 
VITE_USE_MOCK_AZURE_AD=true
VITE_USE_MOCK_AUTH=true
VITE_API_BASE_URL=http://localhost:5000/api
VITE_APP_NAME=Enterprise App (Mock Azure AD)
VITE_APP_VERSION=1.0.0
VITE_ENABLE_RETRY=true
